<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" android:id="@+id/workout_navigation_graph"
    app:startDestination="@id/feedFragment">

    <activity
        android:id="@+id/feedActivity"
        android:name="com.lucianoluzzi.workout.feed.ui.FeedActivity"
        android:label="FeedActivity" />
    <fragment
        android:id="@+id/feedFragment"
        android:name="com.lucianoluzzi.workout.feed.ui.FeedFragment"
        android:label="FeedFragment" >
        <action
            android:id="@+id/action_feedFragment_to_postFragment"
            app:destination="@id/postFragment" />
    </fragment>
    <fragment
        android:id="@+id/postFragment"
        android:name="com.lucianoluzzi.workout.post.ui.PostFragment"
        android:label="PostFragment" />
</navigation>